graph {
   graph [nodesep=2, size="10,10"];
    N0 [shape="box", style="rounded", label="0"];
    N1 [shape="box", style="rounded", label="1"];
    N2 [shape="box", style="rounded", label="2"];
    N3 [shape="box", style="rounded", label="3"];
    N4 [shape="box", style="rounded", label="4"];
    N0 -- N1 [label="4"];
    N0 -- N3 [label="3"];
    N0 -- N4 [label="5"];
    N1 -- N2 [label="2"];
    N2 -- N3 [label="1"];
    overlap=false;
}
